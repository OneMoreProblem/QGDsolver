FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    "(rhoU|rhoE)"
    {
        solver diagonal;
    }
    
    "(rho|U|e)"
    {
        solver   smoothSolver;
        smoother DIC;
        tolerance 1e-9;
        relTol    0;
    }
}


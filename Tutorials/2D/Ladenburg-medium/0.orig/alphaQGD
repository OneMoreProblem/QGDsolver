FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alphaQGD;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.3;

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform 0.3;
    }
    
    outlet
    {
        type            calculated;
        value           uniform 0.3;
    }
    
    freestream
    {
        type            calculated;
        value           uniform 0.3;
    }
    
    freestreamInlet
    {
        type            calculated;
        value           uniform 0.3;
    }
    
    wedge1 {type wedge;}
    wedge2 {type wedge;}
}


